{
  "all": true,
  "check-coverage": true,
  "reporter": ["html", "lcov", "text", "text-summary"],
  "lines": 70,
  "statements": 70,
  "functions": 70,
  "branches": 70,
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "src/**/*.d.ts",
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.ts",
    "src/**/*.spec.tsx",
    "src/**/__tests__/**",
    "src/**/__mocks__/**",
    "src/**/*.stories.tsx"
  ],
  "temp-dir": "./coverage/.nyc_output",
  "report-dir": "./coverage"
}

