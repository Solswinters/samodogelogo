    "next": "14.2.22",
    "react": "^18",
    "react-dom": "^18",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
